{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*).(css|js|html)",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*).(png|jpg|jpeg|gif|svg|otf|ttf|woff|woff2)",
      "dest": "/public/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
